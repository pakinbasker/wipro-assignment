public class Main
{
	public static void main(String[] args) {
		int n1=22,n2=57;
		for (int i=2;i<=(n1/2);i++){
		    if(n1%i==0)
		    {
		        System.out.println("no");
		        break;
		        
		    }
		    	else{	  
		    	    System.out.println("yes"); break;
                }
		    
		}
	}
}

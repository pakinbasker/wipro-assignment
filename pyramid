public class Main
{
	public static void main(String[] args) {
		//System.out.println("Hello World");
    for (int r=1;r<=10;r++){
     
     for (int s=1;s<=10-r;s++){
        System.out.print(" ");
        
    }for (int t=1;t<=r;t++){ System.out.print(t+"");}
        for (int t=r-1;t>=1;t--){
        System.out.print(t+"");}System.out.println();
    }
}



	
}

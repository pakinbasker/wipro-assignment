public class Main
{
	public static void main(String[] args) {
//System.out.println("Hello World");
	
	    int s=0;
	    int a[]={2,3,1,4,39};
	    for(int  i=0;i<a.length;i++){
	        s=s+a[i];
	        
	    }
	    System.out.println("sum of array   "+s);
	    float h=s/a.length;
	    System.out.println(h);
	    
	     
	}
}
